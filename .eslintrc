{
	"extends": "airbnb-base",
	"env": {
		"node": true
	},
	"rules": {
		"import/no-unresolved": 0,
		"import/no-extraneous-dependencies": 0,
		"object-curly-newline": ["error", { "consistent": true }],
		"semi": [2, "never"],
		"semi-style": [2, "first"],
		"camelcase": 0,
		"indent": [2, "tab", { "SwitchCase": 1 }],
		"no-tabs": 0,
		"max-len": 0,
		"no-param-reassign": [2, { "props": false }],
		"func-names": [2, "never"]
	}
}
